# Harvest API Configuration (REQUIRED)
HARVEST_ACCESS_TOKEN=your_harvest_personal_access_token_here
HARVEST_ACCOUNT_ID=your_harvest_account_id_here

# Environment (optional)
NODE_ENV=development
LOG_LEVEL=info

# API Configuration (optional)
HARVEST_API_BASE_URL=https://api.harvestapp.com/v2
REQUEST_TIMEOUT_MS=30000
MAX_RETRIES=3

# Rate Limiting (optional)
RATE_LIMIT_REQUESTS_PER_SECOND=100
RATE_LIMIT_BURST_SIZE=200

# Testing Configuration (optional - for integration tests)
TEST_HARVEST_ACCESS_TOKEN=your_test_token_here
TEST_HARVEST_ACCOUNT_ID=your_test_account_id_here